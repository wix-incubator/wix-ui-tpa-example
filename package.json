  "author": {
    "name": "Lior Belinsky",
    "email": "liorbe@wix.com",
    "url": ""
  },
  "scripts": {
    "start": "haste start",
    "precommit": "haste lint",
    "pretest": "haste build",
    "test": "haste test --jest",
    "posttest": "haste lint",
    "release": "haste release"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/prop-types": "~15.5.0",
    "@types/react": "~15.6.0",
    "@types/react-dom": "~15.5.0",
    "@types/react-i18next": "~4.6.0",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-15": "^1.0.5",
    "express": "~4.15.0",
    "express-session": "~1.15.0",
    "haste-preset-yoshi": "latest",
    "husky": "~0.14.0",
    "stylable-integration": "^6.0.5",
    "react-test-renderer": "~15.6.0",
    "ts-jest": "^22.0.2",
    "typescript": "~2.4.0",
    "velocity": "~0.7.0"
  },
  "dependencies": {
    "axios": "~0.16.0",
    "i18next": "~8.4.0",
    "i18next-xhr-backend": "~1.4.0",
    "prop-types": "~15.6.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-i18next": "~4.8.0",
    "wix-axios-config": "latest",
    "wix-style-processor": "^3.0.121",
    "wix-ui-tpa": "^1.0.47"
  },
  "haste": {
    "preset": "yoshi"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "\\.(css)$": "stylable-integration/jest"
    },
    "transformIgnorePatterns": [],
    "setupTestFrameworkScriptFile": "wix-ui-test-utils/dist/src/jest-setup",
    "setupFiles": [
      "<rootDir>/test/enzyme-setup"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "/src/.*\\.spec\\.(ts|tsx)$"
  },
  "yoshi": {
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM"
    }
  }
}
